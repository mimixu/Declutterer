{
  "manifest_version": 2,

  "name": "General Chrome Declutterer",
  "description": "Removes extraneous information from the DOM",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "declutter.js"]
    }
  ],

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icon.png"
  }
}
